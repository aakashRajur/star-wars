CONTAINER_PORT=5432

CONSUL_HEALTHCHECK_CMD=/usr/local/bin/pg_isready -U ${PG_USER} -d ${PG_DB}
CONSUL_HEALTHCHECK_PROTOCOL=http
CONSUL_HEALTHCHECK_PORT=8000
CONSUL_HEALTHCHECK_URI="${CONTAINER_HOST_NAME}:${CONSUL_HEALTHCHECK_PORT}"
CONSUL_HEALTHCHECK_INTERVAL=1m

PG_USER=wiki
PG_DB=star_wars
PG_BACKUP=/backup.sql
PG_ISREADY_PORT=8000
