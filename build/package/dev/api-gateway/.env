CONTAINER_PORT=8010
DEBUG=false

KAFKA_SERVICE=kafka

CONSUL_SCHEME=http
CONSUL_ADDRESS=star-wars_consul_1:8500
CONSUL_HEALTHCHECK_INTERVAL=1m

HTTP_PORT=8010
HTTP_SSL_CERT=assets/server.crt
HTTP_SSL_KEY=assets/server.key

KAFKA_MAX_PARTITIONS=3
KAFKA_MAX_REPLICAS=3

# trace|debug|info|warning|fatal|fatal|panic
LOG_LEVEL=trace
# text|json
LOG_FORMATTER=text

# DOCKER ENVS
MAIN=cmd/api-gateway/api-gateway.go
DEBUG_PORT=2345