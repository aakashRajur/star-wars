CONTAINER_PORT=8010
DEBUG=false

CONSUL_HEALTHCHECK_PROTOCOL=http
CONSUL_HEALTHCHECK_PORT=8000
CONSUL_HEALTHCHECK_URI=https://${CONTAINER_HOST_NAME}:${CONSUL_HEALTHCHECK_PORT}/stats
CONSUL_HEALTHCHECK_INTERVAL=1m

HTTP_PORT=8010
HTTP_READ_TIMEOUT=5
HTTP_WRITE_TIMEOUT=10
HTTP_IDLE_TIMEOUT=15
HTTP_SSL_CERT=assets/server.crt
HTTP_SSL_KEY=assets/server.key

KAFKA_MAX_PARTITIONS=3
KAFKA_MAX_REPLICAS=3

# trace|debug|info|warning|fatal|fatal|panic
LOG_LEVEL=trace
# text|json
LOG_FORMATTER=text

# DOCKER ENVS
MAIN=cmd/api-gateway.go
DEBUG_PORT=2345